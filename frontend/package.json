{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "cypress:report": "cypress run --reporter mochawesome",
    "cypress:parallel": "cypress-parallel -s cypress:headless -t 4 -d cypress/e2e",
    "cypress:fast": "cypress-parallel -s cypress:headless -t 4 -d cypress/e2e --specsDir cypress/e2e",
    "e2e:fast": "start-server-and-test dev http://localhost:3000 'bash cypress/fast-tests.sh'",
    "e2e:fast-logged": "start-server-and-test dev http://localhost:3000 'bash cypress/fast-tests-with-logs.sh'",
    "cypress:ci": "start-server-and-test dev http://localhost:3000 cypress:report",
    "generate:report": "node scripts/generate-report.js"
  },
