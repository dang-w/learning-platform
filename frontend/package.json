{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:middleware": "node --experimental-vm-modules --no-warnings src/__tests__/test-middleware-direct.js",
    "cypress": "cypress open --config-file e2e-testing/cypress.config.mts",
    "cypress:headless": "cypress run --config-file e2e-testing/cypress.config.mts",
    "e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "e2e:report": "start-server-and-test dev http://localhost:3000 'cypress run --config-file e2e-testing/cypress.config.mts --reporter mochawesome'",
    "e2e:parallel": "start-server-and-test dev http://localhost:3000 'cypress-parallel -s cypress:headless -t 4 -d e2e-testing/cypress/e2e'",
    "e2e:run": "start-server-and-test dev http://localhost:3000 'bash e2e-testing/scripts/run-tests.sh'",
    "e2e:single": "bash e2e-testing/scripts/run-tests.sh",
    "e2e:dashboard": "start-server-and-test dev http://localhost:3000 'bash e2e-testing/scripts/run-with-dashboard.sh'",
    "generate:report": "node e2e-testing/scripts/generate-report.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@tanstack/react-query": "^5.67.3",
    "axios": "^1.8.3",
    "chart.js": "^4.4.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.294.0",
    "next": "15.2.2",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-day-picker": "^9.6.2",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "^2.4.1",
    "react-markdown": "^10.1.0",
    "react-markdown-editor-lite": "^1.3.4",
    "recharts": "^2.15.1",
    "tailwind-merge": "^2.1.0",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/cypress": "^10.0.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.17.25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/testing-library__jest-dom": "^5.14.9",
    "cypress": "^14.2.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-multi-reporters": "^1.6.4",
    "cypress-parallel": "^0.14.0",
    "dotenv": "^16.4.7",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "start-server-and-test": "^2.0.11",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "undici": "^7.5.0"
  }
}
